<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Test1_RequestServlet</servlet-name>
        <servlet-class>servlet.Test1_RequestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test1_RequestServlet</servlet-name>
        <url-pattern>/RS.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test1_getParam</servlet-name>
        <servlet-class>servlet.Test1_getParam</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test1_getParam</servlet-name>
        <url-pattern>/GP.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test2_SessionServlet</servlet-name>
        <servlet-class>servlet.Test2_SessionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test2_SessionServlet</servlet-name>
        <url-pattern>/SS.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test2_getSessionParam</servlet-name>
        <servlet-class>servlet.Test2_getSessionParam</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test2_getSessionParam</servlet-name>
        <url-pattern>/GSP.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test3_ApplicationServlet</servlet-name>
        <servlet-class>servlet.Test3_ApplicationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test3_ApplicationServlet</servlet-name>
        <url-pattern>/AS.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test3_getApplicationServlet</servlet-name>
        <servlet-class>servlet.Test3_getApplicationParam</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test3_getApplicationServlet</servlet-name>
        <url-pattern>/GAP.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test4_Method</servlet-name>
        <servlet-class>servlet.Test4_Method</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test4_Method</servlet-name>
        <url-pattern>/M.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test5_Cookie1</servlet-name>
        <servlet-class>servlet.Test5_Cookie1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test5_Cookie1</servlet-name>
        <url-pattern>/C.servlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Test6_GetCookie</servlet-name>
        <servlet-class>servlet.Test6_GetCookie</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test6_GetCookie</servlet-name>
        <url-pattern>/GC.servlet</url-pattern>
    </servlet-mapping>

    <!--qpplication初始化参数-->
    <context-param>
        <param-name>encoding</param-name>
        <param-value>utf-8</param-value>
    </context-param>

</web-app>