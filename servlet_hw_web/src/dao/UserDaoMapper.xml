<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace是我们定义接口的全限定名字 这样就可以使用接口调用映射的SQL语句了 这个名字一定要和接口对应上-->
<mapper namespace="dao.UserDaoMapper">
    <select id="selectUserByUserName" resultType="bean.User">
        select * from servlet_hw_web_user where username=#{username}
    </select>
    <insert id="insertUser">
        insert into servlet_hw_web_user values(#{id},#{username},#{password},#{phone})
    </insert>
</mapper>